<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="45"
                android:startColor="#FFFFA726"
                android:endColor="#FFF57C00" />
            <stroke
                android:width="3dp"
                android:color="#FFEF6C00" />
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <size
                android:width="18dp"
                android:height="18dp" />
            <solid android:color="#FFFFF3E0" />
        </shape>
    </item>
</layer-list>
